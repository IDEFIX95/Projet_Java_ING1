<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.projet.projet_java.controller.AccueilController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Text layoutX="230.0" layoutY="100.0" text="Bienvenue sur l'application !" />
        <Button fx:id="boutonConnexion" layoutX="250.0" layoutY="150.0" text="Se connecter" onAction="#handleConnexion" />
        <Button fx:id="boutonInscription" layoutX="250.0" layoutY="200.0" text="S'inscrire" onAction="#handleInscription" />
    </children>
</AnchorPane>